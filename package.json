    "codemirror": "^5.60.5",
    "@codemirror/view": "6.24.1",
    "@codemirror/state": "6.4.1",
    "@codemirror/language": "6.10.1",
    "@types/node": "^18.7.13",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^16.2.0",
